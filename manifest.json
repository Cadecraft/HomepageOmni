{
	"name": "Homepage Omni",
	"author": "Cadecraft",
	"version": "0.5.4",
	"description": "A custom homepage/new tab page for using quick links with an omnibar",
	"manifest_version": 3,
	"chrome_url_overrides": {
		"newtab": "homepage.html"
	},
	"permissions": [
		"webNavigation",
		"storage"
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "homepageomni@homepageomni.homepageomni"
		}
	}
}
